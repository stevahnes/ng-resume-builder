{
  "name": "ng-resume-builder",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint --type-check",
    "ci:test": "ng test -c ci",
    "format:check": "npx prettier --list-different \"src/app/**/*.{ts,js,css,json,scss,html,md}\"",
    "format:fix": "npx prettier --write \"src/app/**/*.{ts,js,css,json,scss,html,md}\"",
    "e2e": "ng e2e",
    "build:prod": "ng build --prod",
    "report-coverage": "codecov",
    "ci": "npm run format:check && npm run lint && npm run ci:test",
    "postinstall": "node version.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.9",
    "@angular/common": "~11.0.9",
    "@angular/compiler": "~11.0.9",
    "@angular/core": "~11.0.9",
    "@angular/forms": "~11.0.9",
    "@angular/localize": "~11.0.9",
    "@angular/platform-browser": "~11.0.9",
    "@angular/platform-browser-dynamic": "~11.0.9",
    "@angular/router": "~11.0.9",
    "@ng-bootstrap/ng-bootstrap": "^9.0.0",
    "@ng-select/ng-select": "^6.1.0",
    "bootstrap": "^4.5.0",
    "file-saver": "^2.0.5",
    "fs-extra": "^4.0.2",
    "git-describe": "^4.0.4",
    "jspdf": "^2.3.0",
    "lodash": "^4.17.20",
    "path": "^0.12.7",
    "puppeteer": "^7.0.1",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "~11.0.7",
    "@angular/compiler-cli": "~11.0.9",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.19.16",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.8",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,css,json,scss,html,md}": "prettier --write"
  }
}
